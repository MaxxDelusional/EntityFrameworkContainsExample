@model List<EFExample.Models.Student>
@{
    ViewData["Title"] = "Home Page";
}



@if (Model.Count == 0)
{
    <h4> Query returned no results. Did you seed the database?</h4>
    <h2><a href="@Url.Action("SeedDB", "Home")">Seed Database</a></h2>

}
else
{
    <ul>
        @foreach (var student in Model)
        {
            <li>
                @student.Name
            </li>
        }
    </ul>
}